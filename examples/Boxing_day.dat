&particles
  nsolid = 3,
  diameter = 5000., 500., 50.,
  density = 2300., 2600., 2600., 
  sphericity = 1.0, 1.0, 1.0,
  viscosity = 1.0, 1.0, 1.0,
  specific_heat = 1.2D3, 1.2D3, 1.2D3,
  thermal_conductivity = 2.0D0, 2.D0, 2.D0
/

&numeric
  rungekut = 1,
  omega = 1.2D0,
  optimization = 2,
  maxout = 500
/

