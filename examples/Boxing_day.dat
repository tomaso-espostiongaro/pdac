 ngas = 2
/

&mesh
  nx = 100
  ny = 100
  nz = 100
  itc  = 0,
  iuni = 0,
  grigen = 1,
  center_x = 80800.,
  center_y = 46550.,
  domain_x = 6.0D3,
  domain_y = 5.0D3,
  domain_z = 6.0D3,
  alpha_x = 0.77D0,
  alpha_y = 0.82D0,
  alpha_z = 0.15D0,
  dxmin = 20.D0,
  dxmax = 100.D0,
  dymin = 20.D0,
  dymax = 100.D0,
  dzmin = 20.D0,
  dzmax = 200.D0,
  n0x = 25,
  n0y = 25,
  n0z = 25,
  maxbeta = 1.1D0,
  mesh_partition = 3
